<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="edu.kit.ipd.sdq.eventsim.module">
      <simulation_module
            guice_module="edu.kit.ipd.sdq.eventsim.system.EventSimSystemModule"
            id="edu.kit.ipd.sdq.eventsim.system.module"
            name="System Simulator"
            priority="0">
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.AcquireAction"
               name="AcquireAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.AcquireActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.BranchAction"
               name="BranchAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.BranchActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.ExternalCallAction"
               name="ExternalCallAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.ExternalCallActionStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.ForkAction"
               name="ForkAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.ForkActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.InternalAction"
               name="InternalAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.InternalActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.LoopAction"
               name="LoopAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.LoopActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.ReleaseAction"
               name="ReleaseAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.ReleaseActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.SetVariableAction"
               name="SetVariableAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.SetVariableActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.StartAction"
               name="StartAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.StartActionTraversalStrategy">
         </simulation_strategy>
         <simulation_strategy
               action="org.palladiosimulator.pcm.seff.StopAction"
               name="StopAction"
               strategy="edu.kit.ipd.sdq.eventsim.system.interpreter.strategies.StopActionTraversalStrategy">
         </simulation_strategy>
      </simulation_module>
   </extension>
   <extension
         point="edu.kit.ipd.sdq.eventsim.rvisualization.metriclabels">
      <metric
            label="[System] Response Time"
            name="RESPONSE_TIME_OF_EXTERNAL_CALLS">
      </metric>
      <metric
            label="[System] Execution Time"
            name="TIME_SPAN_BETWEEN_ACTIONS_SYSTEM">
      </metric>
   </extension>


</plugin>
