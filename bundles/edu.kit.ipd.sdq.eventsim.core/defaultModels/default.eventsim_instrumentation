                </probe>
                <probe>
                    <property>resource_demand</property>
                    <probed-type>edu.kit.ipd.sdq.eventsim.instrumentation.description.resource.PassiveResourceRep</probed-type>
                </probe>
                <probe>
                    <property>acquire_time</property>
                    <probed-type>edu.kit.ipd.sdq.eventsim.instrumentation.description.resource.PassiveResourceRep</probed-type>
